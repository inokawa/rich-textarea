{
  "name": "rich-textarea",
  "version": "0.22.2",
  "description": "A small customizable textarea for React to colorize, highlight, decorate texts, offer autocomplete and much more.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "tsc": "tsc -p . --noEmit",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run storybook -- --quiet --no-open\" \"wait-on tcp:6006 && test-storybook\"",
    "typedoc": "typedoc",
    "size": "size-limit",
    "prepublishOnly": "npm run typedoc && rimraf lib && npm run build"
  },
  "dependencies": {
    "range-at-index": "^1.0.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-pure-annotations": "7.22.5",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-terser": "0.4.3",
    "@rollup/plugin-typescript": "11.1.2",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@storybook/addon-docs": "7.0.7",
    "@storybook/react": "7.0.7",
    "@storybook/react-webpack5": "^7.0.5",
    "@storybook/test-runner": "0.10.0",
    "@testing-library/react": "14.0.0",
    "@textlint/kernel": "13.3.2",
    "@textlint/textlint-plugin-text": "13.3.2",
    "@types/node-emoji": "1.8.2",
    "@types/react": "18.2.14",
    "@types/react-highlight-words": "0.16.4",
    "assert": "2.0.0",
    "concurrently": "8.0.1",
    "esbuild": "0.17.19",
    "esbuild-loader": "2.20.0",
    "events": "3.3.0",
    "formik": "2.2.9",
    "jest": "29.0.2",
    "jest-environment-jsdom": "29.0.2",
    "kuromojin": "3.0.0",
    "monaco-diff": "1.2.1",
    "node-emoji": "1.11.0",
    "path-browserify": "1.0.1",
    "prettier": "2.7.1",
    "prism-react-renderer": "2.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-highlight-words": "0.20.0",
    "react-hook-form": "7.43.9",
    "react-is": "18.2.0",
    "rimraf": "5.0.1",
    "rollup": "3.25.1",
    "rollup-plugin-banner2": "1.2.2",
    "size-limit": "^8.2.6",
    "storybook": "^7.0.5",
    "textlint-rule-preset-japanese": "7.0.0",
    "ts-jest": "29.0.5",
    "typedoc": "0.24.8",
    "typedoc-plugin-markdown": "3.15.3",
    "typescript": "5.1.3",
    "wait-on": "7.0.1",
    "webpack": "5.76.0"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/rich-textarea.git"
  },
  "keywords": [
    "react",
    "react-component",
    "textarea",
    "input",
    "form",
    "highlight",
    "autocomplete",
    "tagging",
    "autosize",
    "richtext",
    "editor",
    "contenteditable"
  ],
