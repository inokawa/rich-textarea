{
  "name": "rich-textarea",
  "version": "0.19.0",
  "description": "A small customizable textarea for React to colorize, highlight, decorate texts, offer autocomplete and much more.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "tsc": "tsc -p . --noEmit",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:test": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run storybook -- --quiet --no-open\" \"wait-on tcp:6006 && test-storybook\"",
    "prepublishOnly": "rimraf lib && npm run build"
  },
  "dependencies": {
    "range-at-index": "^1.0.4",
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-pure-annotations": "7.18.6",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-typescript": "8.3.4",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@storybook/react": "6.5.13",
    "@storybook/test-runner": "0.9.0",
    "@testing-library/react": "13.1.1",
    "@textlint/kernel": "12.1.0",
    "@textlint/textlint-plugin-text": "12.1.0",
    "@types/node-emoji": "1.8.2",
    "@types/react": "18.0.24",
    "@types/react-highlight-words": "0.16.4",
    "@types/use-sync-external-store": "0.0.3",
    "asserts": "4.0.2",
    "babel-plugin-replace-import-extensions": "1.0.1",
    "concurrently": "7.2.2",
    "events": "3.3.0",
    "formik": "2.2.9",
    "jest": "29.0.2",
    "jest-environment-jsdom": "29.0.2",
    "kuromojin": "3.0.0",
    "monaco-diff": "1.1.1",
    "node-emoji": "1.11.0",
    "path-browserify": "1.0.1",
    "prettier": "2.7.1",
    "prism-react-renderer": "1.3.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-highlight-words": "0.18.0",
    "react-hook-form": "7.34.2",
    "react-is": "18.1.0",
    "rimraf": "3.0.2",
    "rollup": "2.75.5",
    "rollup-plugin-terser": "7.0.2",
    "textlint-rule-preset-japanese": "7.0.0",
    "ts-jest": "29.0.0",
    "typescript": "4.6.4",
    "wait-on": "6.0.1",
    "webpack": "5.74.0"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/rich-textarea.git"
  },
  "keywords": [
    "react",
    "react-component",
    "textarea",
    "input",
    "highlight",
    "autocomplete",
    "tagging",
    "autosize",
    "richtext",
    "editor",
    "contenteditable"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
